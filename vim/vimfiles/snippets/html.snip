snippet html
  <!DOCTYPE html>
  <html lang="ja">
    <head>
      <meta charset="UTF-8">
      ${1:head_contents}
    </head>
    <body>
      ${2:body_contents}
    </body>
  </html>

snippet scr
  <script src="${1:script_src}"></script>

snippet tmpl
  <script type="text/template" id="${1:template_id}">
    ${2:template_contents}
  </script>
