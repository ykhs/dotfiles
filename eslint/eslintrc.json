{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true,
    "es6": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "strict": 2,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "no-comma-dangle": 0,
    "no-empty": 1,
    "no-empty-class": 0,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-func-assign": 1,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "no-multi-str": 1,
    "handle-callback-err": 1,
    "curly": 0,
    "new-cap": 0,
    "quotes": 0
  }
}
