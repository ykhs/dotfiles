{
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true,
    "es6": true
  },
  "rules": {
    "no-alert": 1,
    "no-comma-dangle": 1,
    "no-console": 0,
    "no-debugger": 1,
    "no-empty": 1,
    "no-empty-class": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-func-assign": 1,
    "no-use-before-define": 1,
    "no-unused-vars": 1,
    "no-shadow": 1,
    "no-underscore-dangle": 0,
    "no-undef": 1,
    "no-multi-str": 1,
    "no-unused-expressions": 1,
    "handle-callback-err": 1,
    "curly": 1,
    "new-cap": 1,
    "quotes": [1, "single"]
  }
}
